{
    "name": "jengo/base",
    "description": "New Jengo based on the current knowledge I have of ci4",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jengo\\Base\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Support\\": "tests/_support/"
        }
    },
    "authors": [
        {
            "name": "Ian Otieno",
            "email": "ochiengian383@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^4.3"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
